pipeline {
    agent any
       stages {
        stage('Verify k6') {
            steps {
                bat 'k6 version'
            }
        }
        stage('Run k6 test') {
            steps {
                bat 'k6 run --vus 2 -- iterations 5 --duration 30s K6_scenarios\\.idea\\test4.js'
            }
        }
    }
}
